BEGIN
    BLOCK "StringFileInfo"
    BEGIN
        BLOCK "040904e4"
        BEGIN
            VALUE "CompanyName", "Codehaus"
            VALUE "FileDescription", "grails binary"
            VALUE "FileVersion", "1.1"
            VALUE "InternalName", "grails"
            VALUE "LegalCopyright", "Copyright (C) 2009"
            VALUE "OriginalFilename", "grails.rc"
            VALUE "ProductName", " grails"
            VALUE "ProductVersion", "1.1"
            VALUE "Comments", "See http://grails.codehaus.org"
        END
    END
    BLOCK "VarFileInfo"
    BEGIN
        VALUE "Translation", 0x409, 1252
    END
END
