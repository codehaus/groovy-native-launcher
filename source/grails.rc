#include <Windows.h>

#define APSTUDIO_READONLY_SYMBOLS

#include <afxres.h>

#undef APSTUDIO_READONLY_SYMBOLS


LANGUAGE LANG_NEUTRAL, SUBLANG_NEUTRAL
#pragma code_page(1252)

grailsicon  ICON  DISCARDABLE "icons/grails - 48x48.ico"

// see http://msdn2.microsoft.com/en-us/library/aa381058.aspx for explanation of the stuff below

